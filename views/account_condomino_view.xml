<odoo>

    <record id="view_condominoo_tree" model="ir.ui.view">
        <field name="name">Condomino tree</field>
        <field name="model">account.condomino</field>
        <field name="inherit_id" ref="base.view_partner_tree" />
        <field name="arch" type="xml">
            <field name="email" position="after">
                <field name="phone" position="move"/>
            </field>
        </field>
    </record>


    <record id="view_condomino_form" model="ir.ui.view">
        <field name="name">Condomino form</field>
        <field name="model">account.condomino</field>
        <field name="inherit_id" ref="base.view_partner_form" />
        <field name="arch" type="xml">
            <field name="type" position="before">
                <field name="condominio_id" />
                <field name="type_condomino" />
            </field>
        </field>
    </record>

    <record model="ir.actions.act_window" id="gcond.action_all_condomini" >
        <field name="name">All Contacts</field>
        <field name="res_model">account.condomino</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem name="Gestione condomini" id="gcond.menu_2_list" parent="gcond.menu_2" 
            action="gcond.action_all_condomini" sequence="10" groups=""/>
</odoo>